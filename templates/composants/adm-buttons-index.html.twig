<div class="buttons">
    <a href="{{ path('administration_'~ elt ~ '_new') }}" class="btn btn-info btn-sm btn-float" title="{{ 'atitle.creer_nouveau'|trans }}" data-provide="tooltip" data-placement="bottom"><i class="ti-plus"></i></a>
    {% if retour is defined %}
        <a href="{{ path(retour.path) }}" class="btn btn-primary btn-float" data-provide="tooltip"
           data-placement="bottom" title="{{ retour.titre }}"><i class="ti-home"></i></a>
    {% else %}
    <a href="{{ path('administration_index') }}" class="btn btn-primary btn-float" data-provide="tooltip"
       data-placement="bottom" title="{{ 'atitle.retour_administration'|trans }}"><span
                class="material-icons">build</span></a>
    {% endif %}
</div>