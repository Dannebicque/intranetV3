{% extends 'base-bornes.html.twig' %}

{% block full_body %}
    <!-- CONTENEUR 1080PX -->
    <div id="container">
        <!-- Date -->
        <div class="borne_date">
            <div class="heure">
                <article class="clock">
                    <div class="hours-container">
                        <div class="hours"></div>
                    </div>
                    <div class="minutes-container">
                        <div class="minutes"></div>
                    </div>
                    <div class="seconds-container">
                        <div class="seconds"></div>
                    </div>
                </article>
            </div>
            <span class="element">{{ datedujourlong(app.request.locale) }}</span>
            <span class="element" style="padding-right:20px;" id="heure"></span>
        </div>
        <!-- /Date -->

        {% for i in 1..2 %}
            {% if zones['zone'~i] is defined %}
                {% set zone = zones['zone'~i] %}
                {% if zone.type == 'semestre' %}
                    {% set nbGroupes = zone.data.semestre.nbgroupeTPEDT %}
                    <!-- HEADER SEMESTRE -->
                    <div class="semestre-header">
                        {% if zone.data.semestre.ordreLmd != 6 %}
                            <span> Semestre {{ zone.data.semestre.ordreLmd }}</span>
                        {% else %}
                            <span>{{ zone.data.semestre.libelle }}</span> {# FIXME : temporaire, pour fix edt LP #}
                        {% endif %}
                    </div>

                    <section class="semestre_borne"
                             style="grid-template-columns: 60px repeat({{ nbGroupes }}, 1fr); height: {% if zones|length >= 2 %} 720px {% else %} 960px {% endif %};" {# FIXME : temporaire, pour fix edt LP #}
                    >
                        {% for j in 1..nbGroupes %}
                            <div class="track-slot col-edt-header jour"
                                 style="grid-area: 1 / {{ j+1 }} / 2 / {{ j+2 }};">
                                <div>{{ zone.data.p1.groupes[j] }}</div>
                            </div>
                            {% if not loop.last %}
                                <div class="dashed-lines-vertical"
                                     style="grid-area: 2 / {{ j+2 }} / 25 / {{ j+2 }}"></div>
                            {% endif %}
                        {% endfor %}

                        <div class="time-slot" style="grid-area: 2 / 1 / 5 / 2;">
                            <span>8h00<br/>-<br/>9h30</span>
                        </div>
                        <div class="time-slot" style="grid-area: 5 / 1 / 8 / 2;">
                            <span>9h30<br/>-<br/>11h00</span>
                        </div>
                        <div class="time-slot" style="grid-area: 8 / 1 / 11 / 2;">
                            <span>11h00<br/>-<br/>12h30</span>
                        </div>
                        <div class="time-slot" style="grid-area: 11 / 1 / 14 / 2;">
                            <span>12h30<br/>-<br/>14h00</span>
                        </div>
                        <div class="time-slot" style="grid-area: 14 / 1 / 17 / 2;">
                            <span>14h00<br/>-<br/>15h30</span>
                        </div>
                        <div class="time-slot" style="grid-area: 17 / 1 / 20 / 2;">
                            <span>15h30<br/>-<br/>17h00</span>
                        </div>
                        <div class="time-slot" style="grid-area: 20 / 1 / 23 / 2;">
                            <span>17h00<br/>-<br/>18h30</span>
                        </div>
                        <div class="time-slot" style="grid-area: 23 / 1 / 25 / 2;">
                            <span>18h30<br/>-<br/>19h30</span>
                        </div>

                        <div class="dashed-lines-horizontal" style="grid-area: 5 / 2 / 8 / {{ nbGroupes + 2 }} "></div>
                        <div class="dashed-lines-horizontal"
                             style="grid-area: 11 / 2 / 14 / {{ nbGroupes + 2 }} "></div>
                        <div class="dashed-lines-horizontal"
                             style="grid-area: 17 / 2 / 20 / {{ nbGroupes + 2 }} "></div>
                        <div class="dashed-lines-horizontal" style="grid-area: 5 / 2 / 23 / {{ nbGroupes + 2 }} "></div>

                        <div class="dashed-lines-horizontal-small"
                             style="grid-area: 3 / 2 / 4 / {{ nbGroupes + 2 }} "></div>
                        <div class="dashed-lines-horizontal-small"
                             style="grid-area: 6 / 2 / 7 / {{ nbGroupes + 2 }} "></div>
                        <div class="dashed-lines-horizontal-small"
                             style="grid-area: 9 / 2 / 10 / {{ nbGroupes + 2 }} "></div>
                        <div class="dashed-lines-horizontal-small"
                             style="grid-area: 12 / 2 / 13 / {{ nbGroupes + 2 }} "></div>
                        <div class="dashed-lines-horizontal-small"
                             style="grid-area: 15 / 2 / 16 / {{ nbGroupes + 2 }} "></div>
                        <div class="dashed-lines-horizontal-small"
                             style="grid-area: 18 / 2 / 19 / {{ nbGroupes + 2 }} "></div>
                        <div class="dashed-lines-horizontal-small"
                             style="grid-area: 21 / 2 / 22 / {{ nbGroupes + 2 }} "></div>

                        {% for j in 1..nbGroupes %}
                            {% for ligne in 0..22 %}
                                {% if zone.data.planning[j][tabHeures[ligne][0]] is defined %}
                                    {% set caseData = zone.data.planning[j][tabHeures[ligne][0]] %}
                                    {% set horsCrenauxWarning = (caseData.gridStart not in crenaux) or (caseData.gridEnd not in crenaux) %}

                                    {% if caseData.ordreGroupe == 41 %}
                                        {% set large = 4 %}
                                    {% else %}
                                        {% set large = largeur[caseData.type_cours] %}
                                    {% endif %}
                                    <div class="session {{ caseData.type_cours|lower }} {{ zone.data.semestre.libelle|lower|slice(0, 2) }}"
                                         style="grid-area:
                                         {{ tabHeuresReversed[caseData.gridStart] + 2 }} /
                                         {{ j+1 }} /
                                         {{ tabHeuresReversed[caseData.gridEnd] + 2 }} /
                                         {{ j + 1 + large }}"
                                    >
                                        <div class="session-body">
                                            <span class="session-title">{{ caseData.texteEvt }}</span>
                                            <span class="session-presenter">{{ caseData.personnel }}</span>
                                            <span class="session-room">{{ caseData.salle }}</span>
                                            {% if horsCrenauxWarning %}
                                                <span style="background-color:crimson;padding: 0.2rem;border-radius: 5px;">
												de {{ caseData.heureDebut|date('H:i') }} à {{ caseData.heureFin|date('H:i') }}
											</span>
                                            {% endif %}
                                        </div>
                                    </div>
                                {% endif %}
                            {% endfor %}
                        {% endfor %}
                    </section>
                {% elseif zone.type == 'message' %}
                    {# <!-- BANDEAU INFOS --> #}
                    {# <section class="bandeau"> #}
                    {# <div class="carousel carousel-slider center" data-indicators="true"> #}
                    {# <div class="carousel-item" style="background-color:#F9B000" href="#sli_0"> #}
                    {# <div class="align"> #}

                    {# <p class="white-text"> #}
                    {# Toutes les informations du département. #}
                    {# </p> #}

                    {# <div class="qrcode"> #}
                    {# <img src="{{ qr_code_data_uri('http://mmi.iut-troyes.univ-reims.fr') }}" /> #}
                    {# <p>Flashez moi pour plus d'informations.</p> #}
                    {# </div> #}
                    {# </div> #}
                    {# </div> #}

                    {# {% for message in messages %} #}
                    {# {% for m in message|filter(m => (m.visible == true)) %} #}
                    {# <div class="carousel-item" style="background-color:{{ m.couleur }}" href="#sli_{{ loop.index }}"> #}
                    {# <div class="align"> #}
                    {# <i class="{{ m.icone }} fa-7x"></i> #}
                    {# <p class="white-text"> #}
                    {# {{ m.message }} #}
                    {# </p> #}
                    {# {% if m.url != '' or m.url != null %} #}
                    {# <div class="qrcode"> #}
                    {# <img src="{{ qr_code_data_uri(m.url) }}" /> #}
                    {# <p>Flashez moi pour plus d'informations.</p> #}
                    {# </div> #}
                    {# {% endif %} #}
                    {# </div> #}
                    {# </div> #}
                    {# {% endfor %} #}
                    {# {% endfor %} #}
                    {# </div> #}
                    {# </section> #}
                    {# <!-- /BANDEAU INFOS --> #}
                {% endif %}
            {% endif %}
        {% endfor %}
        <br>
        <!--Footer-->
        <section class="borne-footer">
            <div style="width:25%; text-align: center; float: left">
                <img src="{{ absolute_url(asset('upload/logo/'~settings('LOGO_IUT'))) }}" alt="logo IUT" width="100px"/>
            </div>
            <div style="width:25%; text-align: center; float: left">
